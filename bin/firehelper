#!/usr/bin/env bb
;; -*- clojure -*-
;; vim: set filetype=clojure:

(require
 '[babashka.classpath :refer [add-classpath]]
 '[clojure.java.io :as io])

(add-classpath (-> (io/file (str *file* "/../../src"))
                   (.getCanonicalPath)))

(require '[firewrap.tool.main :as tool-main])

(apply tool-main/-main *command-line-args*)
